     ]
    }
   ],
   "source": [
    "def md5(string):\n",
    "    \"\"\" 将字符串转成哈希码 \"\"\"\n",
    "    import hashlib\n",
    "    if not isinstance(string, str):\n",
    "        string = str(string)\n",
    "    md5 = hashlib.md5()  \n",
    "    md5.update(string.encode('utf-8'))\n",
    "    return md5.hexdigest()\n",
    "print(md5('password'))"
   ]
